{
  "name": "@gorlikitsme/nosbot.js",
  "version": "0.0.1",
  "description": "A node.js library for building Nostale Bot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "GorlikItsMe",
  "license": "MIT",
  "keywords": [
    "nostale",
    "bot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gorlikitsme/nosbot.js.git"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "yarn build:source && yarn build:declarations",
    "build:source": "rimraf dist/ && tsc -p tsconfig.json",
    "build:declarations": "rimraf declarations/ && tsc -p tsconfig.json --declaration --declarationMap --declarationDir declarations/",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "devDependencies": {
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@zakku/gf-login": "^3.0.2",
    "dll-inject": "^0.0.1",
    "iconv-lite": "^0.6.3",
    "openssl": "^2.0.0",
    "promise-socket": "^7.0.0",
    "wait-for-event": "^2.0.1",
    "winston": "^3.8.2"
  }
}
